generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id              Int      @id @default(autoincrement())
  email           String   @unique
  nombre          String
  apellido        String
  fechaNacimiento DateTime
  ubicacion       String?
  username        String   @unique
  password        String
  createdAt       DateTime @default(now())
}
